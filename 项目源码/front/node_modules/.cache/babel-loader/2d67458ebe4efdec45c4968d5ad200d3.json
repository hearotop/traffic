n<script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        id: '',\r\n        baseUrl: '',\r\n        ro:{\r\n            zhandianmingcheng : false,\r\n            zhandiantupian : false,\r\n            zhandiandizhi : false,\r\n            tujingcheliang : false,\r\n            faburiqi : false,\r\n        },\r\n        type: '',\r\n        userTableName: localStorage.getItem('UserTableName'),\r\n        ruleForm: {\r\n          zhandianmingcheng: '',\r\n          zhandiantupian: '',\r\n          zhandiandizhi: '',\r\n          tujingcheliang: '',\r\n          faburiqi: '',\r\n        },\r\n        rules: {\r\n          zhandianmingcheng: [\r\n            { required: true, message: '站点名称不能为空', trigger: 'blur' },\r\n          ],\r\n          zhandiantupian: [\r\n          ],\r\n          zhandiandizhi: [\r\n            { required: true, message: '站点地址不能为空', trigger: 'blur' },\r\n          ],\r\n          tujingcheliang: [\r\n          ],\r\n          faburiqi: [\r\n          ],\r\n        },\r\n      };\r\n    },\r\n    computed: {\r\n\r\n\r\n\r\n    },\r\n    created() {\r\n\t  //this.bg();\r\n      let type = this.$route.query.type ? this.$route.query.type : '';\r\n      this.init(type);\r\n      this.baseUrl = this.$config.baseUrl;\r\n      this.ruleForm.faburiqi = this.getCurDate()\r\n    },\r\n    methods: {\r\n      getMakeZero(s) {\r\n          return s < 10 ? '0' + s : s;\r\n      },\r\n      // 下载\r\n      download(file){\r\n        window.open(`${file}`)\r\n      },\r\n      // 初始化\r\n      init(type) {\r\n        this.type = type;\r\n        if(type=='cross'){\r\n          var obj = JSON.parse(localStorage.getItem('crossObj'));\r\n          for (var o in obj){\r\n            if(o=='zhandianmingcheng'){\r\n              this.ruleForm.zhandianmingcheng = obj[o];\r\n              this.ro.zhandianmingcheng = true;\r\n              continue;\r\n            }\r\n            if(o=='zhandiantupian'){\r\n              this.ruleForm.zhandiantupian = obj[o].split(\",\")[0];\r\n              this.ro.zhandiantupian = true;\r\n              continue;\r\n            }\r\n            if(o=='zhandiandizhi'){\r\n              this.ruleForm.zhandiandizhi = obj[o];\r\n              this.ro.zhandiandizhi = true;\r\n              continue;\r\n            }\r\n            if(o=='tujingcheliang'){\r\n              this.ruleForm.tujingcheliang = obj[o];\r\n              this.ro.tujingcheliang = true;\r\n              continue;\r\n            }\r\n            if(o=='faburiqi'){\r\n              this.ruleForm.faburiqi = obj[o];\r\n              this.ro.faburiqi = true;\r\n              continue;\r\n            }\r\n          }\r\n        }\r\n        // 获取用户信息\r\n        this.$http.get(this.userTableName + '/session', {emulateJSON: true}).then(res => {\r\n          if (res.data.code == 0) {\r\n            var json = res.data.data;\r\n          }\r\n        });\r\n      },\r\n\r\n    // 多级联动参数\r\n      // 多级联动参数\r\n      info(id) {\r\n        this.$http.get('zhandianxinxi/detail/${id}', {emulateJSON: true}).then(res => {\r\n          if (res.data.code == 0) {\r\n            this.ruleForm = res.data.data;\r\n          }\r\n        });\r\n      },\r\n      // 提交\r\n      onSubmit() {\r\n\r\n        //更新跨表属性\r\n        var crossuserid;\r\n        var crossrefid;\r\n        var crossoptnum;\r\n        this.$refs[\"ruleForm\"].validate(valid => {\r\n          if(valid) {\r\n            if(this.type=='cross'){\r\n                 var statusColumnName = localStorage.getItem('statusColumnName');\r\n                 var statusColumnValue = localStorage.getItem('statusColumnValue');\r\n                 if(statusColumnName && statusColumnName!='') {\r\n                     var obj = JSON.parse(localStorage.getItem('crossObj'));\r\n                     if(!statusColumnName.startsWith(\"[\")) {\r\n                         for (var o in obj){\r\n                             if(o==statusColumnName){\r\n                                 obj[o] = statusColumnValue;\r\n                             }\r\n                         }\r\n                         var table = localStorage.getItem('crossTable');\r\n                         this.$http.post(table+'/update', obj).then(res => {});\r\n                     } else {\r\n                            crossuserid=Number(localStorage.getItem('userid'));\r\n                            crossrefid=obj['id'];\r\n                            crossoptnum=localStorage.getItem('statusColumnName');\r\n                            crossoptnum=crossoptnum.replace(/\\[/,\"\").replace(/\\]/,\"\");\r\n                     }\r\n                 }\r\n            }\r\n            if(crossrefid && crossuserid) {\r\n                 this.ruleForm.crossuserid=crossuserid;\r\n                 this.ruleForm.crossrefid=crossrefid;\r\n                 var params = {\r\n                     page: 1,\r\n                     limit: 10,\r\n                     crossuserid:crossuserid,\r\n                     crossrefid:crossrefid,\r\n                 }\r\n                 this.$http.get('zhandianxinxi/list', {\r\n                  params: params\r\n                 }).then(res => {\r\n                     if(res.data.data.total>=crossoptnum) {\r\n                         this.$message({\r\n                          message: localStorage.getItem('tips'),\r\n                          type: 'success',\r\n                          duration: 1500,\r\n                         });\r\n                          return false;\r\n                     } else {\r\n                         // 跨表计算\r\n\r\n\r\n                          this.$http.post('zhandianxinxi/add', this.ruleForm).then(res => {\r\n                              if (res.data.code == 0) {\r\n                                  this.$message({\r\n                                      message: '操作成功',\r\n                                      type: 'success',\r\n                                      duration: 1500,\r\n                      