ow.chepaihao}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"gengxinshijian\"\n\t\t\t\t\tlabel=\"更新时间\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.gengxinshijian}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column width=\"300\" label=\"操作\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"1px solid #999\",\"cursor\":\"pointer\",\"padding\":\"0 16px\",\"margin\":\"3px 6px 3px 0\",\"outline\":\"none\",\"color\":\"#333\",\"borderRadius\":\"4px\",\"background\":\"none\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\" isAuth('gongjiaoluxian','查看')\" type=\"success\" size=\"mini\" @click=\"addOrUpdateHandler(scope.row.id,'info')\">详情</el-button>\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"1px solid #999\",\"cursor\":\"pointer\",\"padding\":\"0 16px\",\"margin\":\"3px 6px 3px 0\",\"outline\":\"none\",\"color\":\"#333\",\"borderRadius\":\"4px\",\"background\":\"none\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\"isAuth('gongjiaoluxian','维护')\" type=\"success\" size=\"mini\" @click=\"xianluweihuCrossAddOrUpdateHandler(scope.row,'cross','','','')\">维护</el-button>\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"1px solid #7a89d3\",\"cursor\":\"pointer\",\"padding\":\"0 16px\",\"margin\":\"3px 6px 3px 0\",\"outline\":\"none\",\"color\":\"#7a89d3\",\"borderRadius\":\"4px\",\"background\":\"none\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\" isAuth('gongjiaoluxian','修改')\" type=\"primary\" size=\"mini\" @click=\"addOrUpdateHandler(scope.row.id)\">修改</el-button>\n\n\n\n\n\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"1px solid #f8566d\",\"cursor\":\"pointer\",\"padding\":\"0 16px\",\"margin\":\"3px 6px 3px 0\",\"outline\":\"none\",\"color\":\"#f8566d\",\"borderRadius\":\"4px\",\"background\":\"none\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\"isAuth('gongjiaoluxian','删除') \" type=\"danger\" size=\"mini\" @click=\"deleteHandler(scope.row.id)\">删除</el-button>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t</el-table>\n\t\t\t\t<el-pagination\n\t\t\t\t\t@size-change=\"sizeChangeHandle\"\n\t\t\t\t\t@current-change=\"currentChangeHandle\"\n\t\t\t\t\t:current-page=\"pageIndex\"\n\t\t\t\t\tbackground\n\t\t\t\t\t:page-sizes=\"[10, 20, 30, 50]\"\n\t\t\t\t\t:page-size=\"pageSize\"\n\t\t\t\t\t:layout=\"layouts.join()\"\n\t\t\t\t\t:total=\"totalPage\"\n\t\t\t\t\tprev-text=\"<\"\n\t\t\t\t\tnext-text=\">\"\n\t\t\t\t\t:hide-on-single-page=\"true\"\n\t\t\t\t\t:style='{\"width\":\"100%\",\"padding\":\"0\",\"margin\":\"20px 0 0\",\"whiteSpace\":\"nowrap\",\"color\":\"#eee\",\"fontWeight\":\"500\"}'\n\t\t\t\t></el-pagination>\n\t\t\t<!-- </div> -->\n\t\t</template>\n\t\t\n\t\t<!-- 添加/修改页面  将父组件的search方法传递给子组件-->\n\t\t<add-or-update v-if=\"addOrUpdateFlag\" :parent=\"this\" ref=\"addOrUpdate\"></add-or-update>\n\n\t\t<xianluweihu-cross-add-or-update v-if=\"xianluweihuCrossAddOrUpdateFlag\" :parent=\"this\" ref=\"xianluweihuCrossaddOrUpdate\"></xianluweihu-cross-add-or-update>\n\n\n\n\n\t</div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport AddOrUpdate from \"./add-or-update\";\nimport xianluweihuCrossAddOrUpdate from \"../xianluweihu/add-or-update\";\nexport default {\n  data() {\n    return {\n      qishidianOptions: [],\n      zhongzhidianOptions: [],\n      searchForm: {\n        key: \"\"\n      },\n      form:{},\n      dataList: [],\n      pageIndex: 1,\n      pageSize: 10,\n      totalPage: 0,\n      dataListLoading: false,\n      dataListSelections: [],\n      showFlag: true,\n      sfshVisiable: false,\n      shForm: {},\n      chartVisiable: false,\n      chartVisiable1: false,\n      chartVisiable2: false,\n      chartVisiable3: false,\n      chartVisiable4: false,\n      chartVisiable5: false,\n      addOrUpdateFlag: